# Bot Configuration
BOT_TOKEN=your_telegram_bot_token_here

# Webhook Configuration (for production deployment)
USE_WEBHOOK=0
WEBHOOK_URL=https://your-domain.com
WEBHOOK_PATH=/webhook
PORT=8080

# Database (if needed)
REDIS_URL=redis://localhost:6379

# Analytics (if needed)
ANALYTICS_ENABLED=1
METRICS_ENABLED=1

# Debug
DEBUG=1
LOG_LEVEL=INFO
